SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'SÉRIE'; 
SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'SÉRIE' ORDER BY NOME ASC;
SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'SÉRIE' ORDER BY NOME DESC;
SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'FILME';
SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'FILME' AND ANO_LANCAMENTO < 2000; 
SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'SÉRIE' AND ANO_LANCAMENTO > 2015; 
SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'FILME' AND GENERO LIKE '%Ação%';
SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'FILME' AND DURACAO_MINUTOS > 120; 
SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'SÉRIE' AND ANO_LANCAMENTO BETWEEN 2012 AND 2016 ORDER BY ANO_LANCAMENTO, NOME; 
SELECT * FROM ENTRETENIMENTO WHERE TIPO = 'FILME' AND ATOR_PRINCIPAL LIKE '%Brad Pitt%' ORDER BY ANO_LANCAMENTO ASC; 
SELECT NOME, DIRETOR, ATOR_PRINCIPAL FROM ENTRETERIMENTO WHERE GENERO LIKE '%crime%' AND ATOR_PRINCIPAL LIKE '%John Travolta%'; 
SELECT * FROM ENTRETENIMENTO WHERE DURACAO_MINUTOS > 120 AND ATOR_PRINCIPAL NOT LIKE '%Christian Bale%' AND ATOR_PRINCIPAL NOT LIKE '%Tim Robbins%'; 
SELECT * FROM ENTRETENIMENTO WHERE GENERO NOT LIKE '%Comédia%' AND GENERO NOT LIKE '%drama%';
SELECT * FROM ENTRETENIMENTO WHERE ANO_LANCAMENTO NOT IN (2000, 2001, 2002); 
SELECT * FROM ENTRETENIMENTO WHERE ATOR_PRINCIPAL LIKE '%Wagner Moura%' OR ATOR_PRINCIPAL LIKE '%Leonardo DiCaprio%' OR ATOR_PRINCIPAL LIKE '%Keanu Reeves%'; 
